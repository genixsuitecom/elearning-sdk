{
    "name": "@genixsuitecom/elearning-sdk",
    "version": "0.3.1",
    "description": "GenixSuite Learn API TypeScript SDK",
    "keywords": [
        "openapi",
        "sdk",
        "genixsuite",
        "elearning",
        "powerpoint",
        "pdf",
        "txt",
        "video",
        "ai",
        "ai video generation",
        "ai video",
        "ai video creation",
        "training",
        "training content",
        "training documentation",
        "training guides",
        "training manuals",
        "training presentations",
        "training slides",
        "training videos"
    ],
    "homepage": "https://github.com/genixsuitecom/elearning-sdk/tree/HEAD/sdk/ts#readme",
    "bugs": {
        "url": "https://github.com/genixsuitecom/elearning-sdk/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/genixsuitecom/elearning-sdk.git",
        "directory": "sdk/ts"
    },
    "license": "MIT",
    "author": "GenixSuite.com",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "elearning-sdk": "./dist/examples/doctor.js"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "clean": "rimraf dist || rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build",
        "typecheck": "tsc -p tsconfig.json --noEmit",
        "test:types": "tsd",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix"
    },
    "devDependencies": {
        "@types/node": "^24.9.2",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^8.57.0",
        "eslint-plugin-promise": "^6.1.1",
        "rimraf": "^6.0.1",
        "tsd": "^0.31.2",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public"
    },
    "module": "dist/index.js",
    "sideEffects": false,
    "dependencies": {
        "dotenv": "^17.2.3",
        "zod": "^4.1.12"
    }
}
